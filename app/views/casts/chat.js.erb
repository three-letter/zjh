
<% broadcast "/poker/#{@cast.id}" do %>
  <% unless @text.blank? %>
    $("#zjh_reports").append("<li>[<%= Time.now.strftime("%H:%M:%S") %>] <font color=''><%= @current_user.name %>: <%= @text %> </li>");
  <% end %>
<% end %>

<% if @text.blank? %>
  alert("<%= @msg %>"); 
<% end %>

