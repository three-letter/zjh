
<% @seat_pokers.each do |uid, pokers| %>
  <% binding.pry %>
  <% broadcast "/room/#{@room.id}/user/#{uid}" do %>
    $("#poker_title").show();
    $("#poker_area").html("<%= escape_javascript(render(:partial => 'seats/poker', :locals => {:pokers => pokers})) %>");
    <% if uid == @room.sort_seats[0].user.id %>
      $(".poker_play").show();
    <% end %>
  <% end %>
<% end %>
